{
  "name": "promptty",
  "version": "0.1.0",
  "description": "Slack/Teams to LLM CLI bridge",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@slack/bolt": "^4.1.0",
    "botbuilder": "^4.23.1",
    "botframework-connector": "^4.23.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.7.2"
  }
}
